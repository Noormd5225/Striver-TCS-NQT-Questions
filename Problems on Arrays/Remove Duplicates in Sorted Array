    public static void RemoveDuplicates(){
        int [] nums={0,0,0,0,1,1,1,2,3,3,3,3,4,4,4,4};

        int left=0;
        for(int x:nums){
            if(nums[left]!=x){
                nums[++left]=x;
            }
        }
        for(int i=0;i<=left;i++){
            System.out.println(nums[i]);
        }
    }
