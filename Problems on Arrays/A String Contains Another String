  public static boolean RotateOfAnother(){
        String s = "abcde", goal = "cdeab";
        String str=s+s;
        int j=0;
        for(int i=0;i<str.length()&&j<goal.length();i++){
            if(str.charAt(i)==goal.charAt(j)){
                j++;
            }
            else {
                j=0;
            }
            //   System.out.println(str.charAt(i));
        }
        if(j==goal.length()){
            return true;
        }


        return false;
    }
