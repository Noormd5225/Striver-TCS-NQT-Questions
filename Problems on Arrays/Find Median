 public static void FindMedian(){
        int [] nums={1,2,24,3,4,9,53,34};
        if(nums.length%2==0){
            int sum=0;
            for(int x:nums){
                sum+=x;
            }
            System.out.println("the meidan value is:"+sum/nums.length);
        }
        else{
            Arrays.sort(nums);
            System.out.println(nums[nums.length/2]);
        }
    }
